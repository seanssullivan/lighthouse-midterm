.ordersList
  h3.ordersList__title ORDERS
  .underline
  ul.ordersList__list
    li.ordersList__item
      span.ordersList__item--id ID
      span.ordersList__item--timeStart TIME AGO
    for order in data.orders
      <li class="ordersList__item">
        span.ordersList__item--id= order.id
        span.ordersList__item--timeStart= order.ordered_at
      </li>
  .ordersList__index
    i(class="ordersList__index--prev fas fa-arrow-left")
    span.ordersList__index--text PAGE
    span.ordersList__index--page 1/25
    i(class="ordersList__index--next fas fa-arrow-right")