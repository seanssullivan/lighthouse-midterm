.ordersCompletedList
  h3.ordersCompletedList__title ORDERS
  .underline
  .ordersCompletedList__listTitle
    span.ordersCompletedList__listTitle--id ID
    span.ordersCompletedList__listTitle--day Day
    span.ordersCompletedList__listTitle--total Total
  ul.ordersCompletedList__list
    for order in data.orders
      - var arrIndex = 0;
      - var days = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'];
      - var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December']
      - var day = new Date(order.ordered_at).getDay()
      - var month = new Date(order.ordered_at).getMonth()
      - var year = new Date(order.ordered_at).getFullYear()
      <li class="ordersCompletedList__item">
        span.ordersCompletedList__item--id= order.id
        span.ordersCompletedList__item--timeStart= days[day] + ' ' + months[month] + ' ' + year
        span.ordersCompletedList__item--price= '$' + order.total_cost
      </li>
      - arrIndex++
  .ordersCompletedList__index
    i(class="ordersCompletedList__index--prev fas fa-arrow-left")
    span.ordersCompletedList__index--text PAGE
    span.ordersCompletedList__index--page= data.page + '/25'
    i(class="ordersCompletedList__index--next fas fa-arrow-right")